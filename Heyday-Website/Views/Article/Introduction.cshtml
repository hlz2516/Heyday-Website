@model IEnumerable<Heyday_Website.Models.Article>

<div class="container">
    <a class="btn btn-light float-left" asp-controller="Article" asp-action="NewArticle">新建文章</a>
    <br />
    <ul class="list-group list-group-flush">
        @foreach (var article in Model)
        {
        <li class="list-group-item">
            <a class="float-left">@article.Title</a>
            <a class="float-right mr-2">删除</a>
            <a class="float-right mr-2">编辑</a>
        </li>
        }
    </ul>
</div>
